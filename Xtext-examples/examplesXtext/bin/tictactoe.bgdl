Title "Tic-Tac-Toe"
BoardSize 3

ValidMove placePiece{
	placeAnywhere
}

PieceType X {
	Symbol "❌"
	ValidMoves placePiece
}

PieceType O { 
	Symbol "⭕"
	ValidMoves placePiece
}

WinCondition {
	Line length 3 direction ROW
	Line length 3 direction COLUMN
	Line length 3 direction DIAGONAL
}
			  
			  
